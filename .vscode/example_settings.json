        "**/*pyc": true
    },
    //"python.analysis.logLevel": "Trace",
    //"files.autoSave": "afterDelay",
    //"makefile.extensionOutputFolder": "./.vscode",
    "files.associations": {
        "panther_analysis_tool": "python"
    }
}